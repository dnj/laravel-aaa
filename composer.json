{
	"name": "dnj/aaa",
	"license": "MIT",
	"minimum-stability": "dev",
	"prefer-stable": true,
	"require": {
		"php": "^8.1",
		"laravel/framework": "^9",
		"laravel/passport": "^11.2"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.5",
		"phpstan/phpstan": "^1.4.1",
		"friendsofphp/php-cs-fixer": "^3.1"
	},
	"autoload": {
		"psr-4": {
			"dnj\\AAA\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"dnj\\AAA\\Tests\\": "tests/"
		}
	},
	"scripts": {
		"test:types": "vendor/bin/phpstan analyze",
		"test:phpunit": "vendor/bin/phpunit",
		"test:codestyle": "vendor/bin/php-cs-fixer fix -v --dry-run --stop-on-violation --using-cache=no",
		"test": [
			"@test:types",
			"@test:phpunit",
			"@test:codestyle"
		]
	},
	"extra": {
		"laravel": {
			"providers": [
				"dnj\\AAA\\AAAServiceProvider"
			]
		}
	}
}
